doctype
html(lang='en')
  head
    meta(charset="UTF-8")
    title= wp_title
    link(rel="stylesheet" type="text/css" href= theme.link . "/style.css" media="screen")
    =wp_head

  body

    header
      a(href= site.url)= site.name
      include ./nav

    block content

    include ./sidebar

    =wp_footer
